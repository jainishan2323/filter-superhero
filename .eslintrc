{
  "parser": "babel-eslint",
  "env": {
    "es6":     true,
    "browser": true,
    "node":    true,
    "mocha":   true
  },
  "plugins": [
    "react",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "strict": 0,
    "quotes": [1, "single"],
    "no-underscore-dangle": 0,
    "no-unused-vars": [1, {"varsIgnorePattern": "PropTypes"}],
    "no-unused-expressions": 0,
    "react/prop-types": 1,
    "react/no-unused-prop-types": 1,
    "react/jsx-no-undef": 2,
    "new-cap": 0,
    "yoda": 0,
    "no-empty": 0,
    "no-extra-boolean-cast": 0,
    "consistent-return": 0,
    "react/no-unescaped-entities": 1,
    "no-alert": 1,
    "no-console": 1
  }
}
